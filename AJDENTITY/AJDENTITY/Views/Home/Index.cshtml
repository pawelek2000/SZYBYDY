@model IEnumerable<AJDENTITY.Models.Announcement>

@{
    ViewBag.Title = "Index";
}

<div class="jumbotron">
    <h1>WIRTUALNY DZIENNIK SZKOLNY</h1>   
</div>

<div class="row">

        @foreach (var item in Model)
        {

            <div class="col-md-4">
                <h2>@Html.DisplayFor(modelItem => item.Title)</h2>
                <p>
                    @Html.DisplayFor(modelItem => item.Content)
                </p>
            </div>
        
        }
</div>